<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="QueryByIdentifier"
    nsURI="http://www.furcas.org/TCS/featuretests/property/queryByIdentifier">
  <eClassifiers xsi:type="ecore:EClass" name="Definition">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Usage">
    <eStructuralFeatures xsi:type="ecore:EReference" name="boundDefinition" lowerBound="1"
        eType="#//Definition"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parentList" eType="#//DefinitionUsageList"
        eOpposite="#//DefinitionUsageList/usages"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DefinitionUsageList">
    <eStructuralFeatures xsi:type="ecore:EReference" name="definitions" upperBound="-1"
        eType="#//Definition"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="usages" upperBound="-1"
        eType="#//Usage" eOpposite="#//Usage/parentList"/>
  </eClassifiers>
</ecore:EPackage>
