<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="com.sap.furcas.metamodel.TCS.presentation.TCSModelWizardID"
            name="%_UI_TCSModelWizard_label"
            class="com.sap.furcas.metamodel.TCS.presentation.TCSModelWizard"
