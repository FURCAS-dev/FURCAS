<?xml version="1.0" encoding="UTF-8"?> 
<?eclipse version="3.4"?> 
<plugin> 
	<extension point="org.eclipse.ui.editors"> 
		<editor 
			class="bibtex.dsl.editor.BibtexEditor"
			extensions="bib" 
			id="bibtex.dsl.editor.BibtexEditor" 
			name="Bibtex Editor"> 
		</editor> 
	</extension>
 <extension
       point="com.sap.ide.cts.parser.parserFactory">
    <configuration
          dynamicParserFactoryClass="bibtex.dsl.parser.BibtexParserFactory"
          languageID="bibtex">
    </configuration>
 </extension>
 <extension
       point="com.sap.furcas.ide.editor.autoEditing">
    <configuration
          autoEditingType="static (configurable)"
          autoIndentBraces="{,}">
    </configuration>
 </extension>
 <extension
       point="com.sap.furcas.ide.editor.bracketMatching">
    <configuration
          matchingBrackets="{}[]()"
          matchingType="configurable">
    </configuration>
 </extension>
 <extension
       point="com.sap.furcas.ide.editor.folding">
    <configuration
          foldingType="static (configurable)"
          staticFoldingBrackets="{,}">
    </configuration>
 </extension>
 <extension point="com.sap.furcas.runtime.referenceresolving.furcas_syntax">
       <furcas_syntax provider_class="bibtex.dsl.editor.Activator">
       	 <metamodel nsURI="http://www.sap.com/furcas/examples/bibtex" />
       </furcas_syntax>
 </extension> 
 
</plugin> 
