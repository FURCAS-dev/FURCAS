<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.furcas.org/oclinversion"
            class="com.sap.furcas.oclinversion.OclinversionPackage"
            genModel="model/olcinversion.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
            type="oclinversion"
            class="com.sap.furcas.oclinversion.util.OclinversionResourceFactoryImpl"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.furcas.org/oclinversion/oclequation"
            class="com.sap.furcas.oclinversion.oclequation.OclequationPackage"
            genModel="model/olcinversion.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
            type="oclequation"
            class="com.sap.furcas.oclinversion.oclequation.util.OclequationResourceFactoryImpl"/>
   </extension>

</plugin>
