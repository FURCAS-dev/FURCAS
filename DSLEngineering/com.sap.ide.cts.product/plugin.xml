<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.core.runtime.products">
      <product
            application="com.sap.ide.cts.product"
            name="FURCAS">
      </product>
   </extension>
   <extension
         id="FURCAS"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="FURCAS">
         <property
               name="windowImages"
               value="images\furclipse_transparent_16.png,images\furclipse_transparent_32.png,images\furclipse_transparent_48.png,images\furclipse_transparent_64.png,images\furclipse_transparent_128.png">
