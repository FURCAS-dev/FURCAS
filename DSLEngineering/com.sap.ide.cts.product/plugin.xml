         <property
               name="aboutText"
               value="%productBlurb">
         </property>
         <property
               name="aboutImage"
               value="images/about.png">
         </property>
         <property
               name="startupForegroundColor"
               value="C0C0C0">
         </property>
         <property
               name="startupProgressRect"
               value="0,150,455,15">
         </property>
         <property
               name="startupMessageRect"
               value="7,130,240,20">
         </property>
         <property
               name="appName"
               value="FURCAS">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension>

</plugin>
