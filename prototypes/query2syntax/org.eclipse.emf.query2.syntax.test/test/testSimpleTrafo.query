import "platform:/resource/org.eclipse.emf.query2.syntax.test/model/Ecore.ecore" 

SelectEClass : select a from EClass as a

SelectEClassWhereNameEqualsEAttribute : select a from EClass as a where a.name = 'EAttribute'

SelectAttrTwoWhereAnd: select a.name from EClass as a where a.name = 'EAttribute' and a.abstract = true

SelectAttrWhereAttrLong: select a.lowerBound from ETypedElement as a where a.upperBound < 5

SelectAttrWhereAttrVar: select a.lowerBound, b.upperBound from ETypedElement as a, ETypedElement as b where b.upperBound >= a.lowerBound

SelectAttrWhereAttrNull: select a.lowerBound from ETypedElement as a where a.lowerBound = null

SelectEClassWhereRefNull: select a from EClass as a where a.eOperations = null

SelectAttrTwoWhereOr: select a.name from EClass as a where a.name = 'EAttribute' or a.abstract = true

SelectAttrThreeWhereOr: select a.name from EClass as a where a.name = 'EAttribute' or a.abstract = true or a.abstract = true or a.interface = true

SelectAttrThreeWhereAnd: select a.name from EClass as a where a.name = 'EAttribute' and a.abstract = true and a.abstract = true and a.interface = true 

SelectAttrThreeWhereOrAnd: select a.name from EClass as a where a.name = 'EAttribute' or a.abstract = true and a.interface = true

SelectPar: select a.name from EClass as a where (a.name = 'EAttribute' or a.abstract = true) and a.interface = true

SelectPar2: select a.name from EClass as a where a.name = 'EAttribute' or (a.abstract = true and a.interface = true)