import "platform:/plugin/org.eclipse.emf.query2.syntax.test/model/Ecore.ecore"

SelectEClass:
from EClass as a
select a

SelectEClassWhereNameEqualsEAttribute:
from EClass as a
select a
where a.name = 'EAttribute'

SelectAttrTwoWhereAnd:
from EClass as a
select a.name
where a.name = 'EAttribute' and a.abstract = true

SelectAttrWhereAttrLong:
from ETypedElement as a
select a.lowerBound
where a.upperBound < 5

SelectAttrWhereAttrVar:
from ETypedElement as a, ETypedElement as b
select a.lowerBound, b.upperBound
where b.upperBound >= a.lowerBound

SelectAttrWhereAttrNull:
from ETypedElement as a
select a.lowerBound
where a.eType = null

SelectEClassWhereRefNull:
from EClass as a
select a
where a.eOperations = null

SelectAttrTwoWhereOr:
from EClass as a
select a.name
where a.name = 'EAttribute' or a.abstract = true

SelectAttrThreeWhereOr:
from EClass as a
select a.name
where a.name = 'EAttribute' or a.abstract = true or a.abstract = true or a.interface = true

SelectAttrThreeWhereAnd:
from EClass as a
select a.name
where a.name = 'EAttribute' and a.abstract = true and a.abstract = true and a.interface = true

SelectAttrThreeWhereOrAnd:
from EClass as a
select a.name
where a.name = 'EAttribute' or a.abstract = true and a.interface = true

SelectPar:
from EClass as a
select a.name
where ( a.name = 'EAttribute' or a.abstract = true ) and a.interface = true

SelectPar2:
from EClass as a
select a.name
where a.name = 'EAttribute' or ( a.abstract = true and a.interface = true )

SelectPar2AsParameter:
SelectPar2:
from EClass as a
select a.name
where a.name = ? or ( a.abstract = ? and a.interface = ? )