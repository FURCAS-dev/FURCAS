<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://sap.com/moin/xm/serializer" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://sap.com/moin/xm/serializer C:\japro\moin_dev\moin\moin.base\dev\src\_com.sap.tc.moin.xm\libm\core\schemas\SerializerMapping.xsd">
	<!--
		tests filter
	-->
	<namespaces>
		<namespace uri="http://sap.com/moin/xm/test/Person" prefix="pp" />
	</namespaces>
	<defaultContainer>sap.com/tc/moin/testcases/mc</defaultContainer>
	<defaultPackagePath>mc.test</defaultPackagePath>
	<instance class="Company">
		<xmlElement name="company" prefix="pp">
			<link association="CustomersOrEmployeesOfCompany" endOutputName="persons">
				<filter class="Employee">
					<xmlElement name="employee" prefix="pp">
						<filter class="Employee">
							<mappingCall id="person" />
						</filter>
						<link association="EquipmentsOfEmployee" endOutputName="equipments">
							<xmlElement name="equipment" prefix="pp">
								<attribute name="id">
									<xmlAttribute name="id" />
								</attribute>
							</xmlElement>
						</link>
					</xmlElement>
				</filter>
				<filter class="Customer">
					<xmlElement name="customer" prefix="pp">
						<mappingCall id="person" />
					</xmlElement>
				</filter>
			</link>
		</xmlElement>
	</instance>
	<mapping id="person">
		<attribute name="name">
			<xmlElement name="name" prefix="pp">
				<putValue />
			</xmlElement>
		</attribute>
		<attribute name="title">
			<xmlElement name="title" prefix="pp">
				<putValue />
			</xmlElement>
		</attribute>
		<link association="AddressesOfPerson" endOutputName="addresses">
			<xmlElement name="address" prefix="pp">
				<attribute name="town">
					<xmlElement name="town" prefix="pp">
						<putValue />
					</xmlElement>
				</attribute>
				<link association="StreetOfAddress" endOutputName="street">
					<xmlElement name="street" prefix="pp">
						<attribute name="name">
							<xmlElement name="name" prefix="pp">
								<putValue />
							</xmlElement>
						</attribute>
						<attribute name="number">
							<xmlElement name="number" prefix="pp">
								<putValue />
							</xmlElement>
						</attribute>
					</xmlElement>
				</link>
			</xmlElement>
		</link>
	</mapping>
</mappings>