<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

<!-- ======================================================================================= -->
<!-- Extension Points               	                                                     -->
<!-- ======================================================================================= -->
   <extension-point id="editorregistry" name="%editorregistry_name" schema="schema/editorregistry.exsd"/>
   <extension-point id="oclQuickfix" name="%oclQuickfix_name" schema="schema/oclQuickfix.exsd"/>
   <extension-point id="modelObjectEditorAssociations" name="%modelObjectEditorAssociations_name" schema="schema/modelObjectEditorAssociations.exsd"/>

<!-- ======================================================================================= -->
<!-- View Categories      		                                                             -->
<!-- ======================================================================================= -->
<!-- "Modeling Infrastructure" -->
   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.sap.mi.views.moin"
            name="%category.name.views.moin"/>
   </extension>

<!-- ======================================================================================= -->
<!-- Ui Hook into com.sap.mi.fwk                                                             -->
<!-- ======================================================================================= -->
   <extension
         point="com.sap.mi.fwk.uiHook">
      <commandExecutor
            class="com.sap.mi.fwk.ui.internal.MiFwkUiExtensionFactory:com.sap.mi.fwk.commands.ICommandExecutor">
      </commandExecutor>
      <operationExecutor
            class="com.sap.mi.fwk.ui.internal.MiFwkUiExtensionFactory:com.sap.mi.fwk.internal.uihook.IMiFwkOperationExecutor">
      </operationExecutor>
   </extension>

<!-- ======================================================================================= -->
<!-- Editor Launcher			                                                             -->
<!-- ======================================================================================= -->
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.sap.mi.fwk.ui.internal.ModelEditorLauncher"
            icon="resources/metamodelWorkingSet.gif"
            id="com.sap.mi.fwk.ui.editor.EditorLauncher"
            name="%editor.name.launcher">
         This editor is not associated with any file extension since it is only used to start editors from markers displayed in the problems or tasks view.
      </editor>
   </extension>
   
<!-- ======================================================================================= -->
<!-- Adapter Factories			                                                             -->
<!-- ======================================================================================= -->
    <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="com.sap.tc.moin.repository.ide.metamodels.MmDeploymentInfo"
            class="com.sap.mi.fwk.ui.internal.mm.MmPropertiesAdapterFactory">
         <adapter type="org.eclipse.ui.views.properties.IPropertySource"/>
      </factory>
      <factory
            adaptableType="com.sap.tc.moin.repository.ide.metamodels.MmDeploymentInfo"
            class="com.sap.mi.fwk.ui.internal.mm.MmPropertiesAdapterFactory">
         <adapter type="org.eclipse.ui.views.properties.IPropertySource"/>
      </factory>
      <factory
            adaptableType="com.sap.tc.moin.repository.mmi.reflect.RefObject"
            class="com.sap.mi.fwk.ui.internal.MiFwkUiExtensionFactory">
         <adapter type="org.eclipse.ui.views.properties.IPropertySource"/>
      </factory>
      <factory
            adaptableType="com.sap.mi.fwk.ui.tree.nodes.ITreeNodeRefObject"
            class="com.sap.mi.fwk.ui.internal.tree.TreeNodeRefObjectAdapterFactory">
         <adapter type="com.sap.tc.moin.repository.mmi.reflect.RefObject"/>
         <adapter
               type="org.eclipse.ui.views.tasklist.ITaskListResourceAdapter">
         </adapter>
      </factory> 
   </extension>
 
<!-- ======================================================================================= -->
<!-- Element Factories			                                                             -->
<!-- ======================================================================================= -->
      <extension
            point="org.eclipse.ui.elementFactories">
         <factory
               class="com.sap.mi.fwk.ui.editor.ModelObjectElementFactory"
               id="com.sap.mi.fwk.ui.editor.ModelObjectElementFactory"/>
         <factory
               class="com.sap.mi.fwk.ui.internal.mm.MmElementFactory"
               id="com.sap.mi.fwk.ui.metamodelFactory"/>
      </extension>
      
<!-- ======================================================================================= -->
<!-- Working Sets				                                                             -->
<!-- ======================================================================================= -->
      <extension
            point="org.eclipse.ui.workingSets">
         <workingSet
               icon="resources/metamodelWorkingSet.gif"
               id="com.sap.mi.fwk.ui.metamodelWorkingSet"
               name="%workingset.name.metamodels"
               pageClass="com.sap.mi.fwk.ui.internal.mm.MmWorkingSetPage"/>
      </extension>

<!-- ======================================================================================= -->
<!-- Marker Resolutions			                                                             -->
<!-- ======================================================================================= -->
   <extension
         point="org.eclipse.ui.ide.markerResolution">
      <markerResolutionGenerator
            class="com.sap.mi.fwk.ui.internal.constraints.ResolutionGenerator"
            markerType="com.sap.mi.fwk.oclproblemmarker"/>
      <markerResolutionGenerator
            class="com.sap.mi.fwk.ui.internal.consistency.ConsistencyResolutionGenerator"
            markerType="com.sap.mi.fwk.consistencyviolationmarker">
      </markerResolutionGenerator>
   </extension>
 
<!-- ======================================================================================= -->
<!-- Marker Help				                                                             -->
<!-- ======================================================================================= -->
   <extension
         point="org.eclipse.ui.ide.markerHelp">
      <markerHelp
            helpContextId="com.sap.mi.fwk.ui.DuplicateElementConsistencyViolation"
            markerType="com.sap.mi.fwk.consistencyviolationmarker">
         <attribute
               name="com.sap.mi.fwk.violationType"
               value="DuplicateElementConsistencyViolation">
         </attribute>
      </markerHelp>
      <markerHelp
            helpContextId="com.sap.mi.fwk.ui.DanglingLinkConsistencyViolation"
            markerType="com.sap.mi.fwk.consistencyviolationmarker">
         <attribute
               name="com.sap.mi.fwk.violationType"
               value="DanglingLinkConsistencyViolation">
         </attribute>
      </markerHelp>
      <markerHelp
            helpContextId="com.sap.mi.fwk.ui.DuplicateParentConsistencyViolation"
            markerType="com.sap.mi.fwk.consistencyviolationmarker">
         <attribute
               name="com.sap.mi.fwk.violationType"
               value="DuplicateParentConsistencyViolation">
         </attribute>
      </markerHelp>
   </extension>
   
<!-- ======================================================================================= -->
<!-- Help Contexts				                                                             -->
<!-- ======================================================================================= -->
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="helpContexts.xml">
      </contexts>
   </extension>
   
<!-- ======================================================================================= -->
<!-- Commands					                                                             -->
<!-- ======================================================================================= -->
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.eclipse.ui.category.project"
            description="%action.descr.refreshdata"
            id="com.sap.mi.fwk.ui.commands.refreshdata"
            name="%action.name.refreshdata">
      </command>
      <command
            categoryId="org.eclipse.ui.category.project"
            description="%action.descr.refreshclientspec"
            id="com.sap.mi.fwk.ui.commands.refreshclientspec"
            name="%action.name.refreshclientspec">
      </command>
      <command
            categoryId="org.eclipse.ui.category.project"
            description="%action.descr.deletedata"
            id="com.sap.mi.fwk.ui.commands.deletedata"
            name="%action.name.deletedata">
      </command>
      <category
            description="%category.descr.commands.metamodel"
            id="com.sap.mi.fwk.ui.commands.categories.metamodel"
            name="%category.name.commands.metamodel">
      </category>
   </extension>

<!-- ======================================================================================= -->
<!-- Handlers					                                                             -->
<!-- ======================================================================================= -->
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="com.sap.mi.fwk.ui.internal.RefreshModelDataAction"
            commandId="com.sap.mi.fwk.ui.commands.refreshdata">
         <activeWhen>
            <reference
                  definitionId="com.sap.mi.fwk.expressions.moinProjects">
            </reference>
         </activeWhen>
      </handler>
      <handler
            class="com.sap.mi.fwk.ui.internal.RefreshClientSpecAction"
            commandId="com.sap.mi.fwk.ui.commands.refreshclientspec">
         <activeWhen>
            <and>
               <count
                     value="1">
               </count>
	            <reference
	                  definitionId="com.sap.mi.fwk.expressions.moinProjects">
	            </reference>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="com.sap.mi.fwk.ui.internal.DeleteAllModelDataAction"
            commandId="com.sap.mi.fwk.ui.commands.deletedata">
         <activeWhen>
            <reference
                  definitionId="com.sap.mi.fwk.expressions.moinProjects">
            </reference>
         </activeWhen>
      </handler>
   </extension>

<!-- ======================================================================================= -->
<!-- Menus						                                                             -->
<!-- ======================================================================================= -->
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
	<!-- Modeling Infrastructure -->
         <menu
               id="com.sap.mi.fwk.ui.menu.modeling"
               label="%menu.name.modeling"
               mnemonic="%mnemonic.menu.modeling">
            <visibleWhen>
               <reference
                     definitionId="com.sap.mi.fwk.expressions.moinProjects">
               </reference>
            </visibleWhen>
            <separator
                  name="group.refresh"
                  visible="false">
            </separator>
            <separator
                  name="group.partition"
                  visible="true">
            </separator>
            <separator
                  name="group.partition.end"
                  visible="false">
            </separator>
            <separator
                  name="group.project"
                  visible="true">
            </separator>
            <separator
                  name="additions"
                  visible="true">
            </separator>
         </menu>
      </menuContribution>
	<!-- Refresh Model Data-->
      <menuContribution
            locationURI="popup:org.eclipse.ui.navigator.ProjectExplorer#PopupMenu?after=group.build">
         <command
               commandId="com.sap.mi.fwk.ui.commands.refreshdata"
               label="%action.name.refreshdata"
               mnemonic="%mnemonic.action.refreshdata">
            <visibleWhen>
               <reference
                     definitionId="com.sap.mi.fwk.expressions.moinProjects">
               </reference>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="popup:com.sap.mi.fwk.ui.menu.modeling?after=group.refresh">
         <command
               commandId="com.sap.mi.fwk.ui.commands.refreshdata"
               id="com.sap.mi.fwk.ui.menuItems.refreshdata"
               label="%action.name.refreshdata"
               mnemonic="%mnemonic.action.refreshdata">
         </command>
      </menuContribution>
	<!-- Refresh ClientSpec-->
      <menuContribution
            locationURI="popup:com.sap.mi.fwk.ui.menu.modeling?after=com.sap.mi.fwk.ui.menuItems.refreshdata">
         <command
               commandId="com.sap.mi.fwk.ui.commands.refreshclientspec"
               id="com.sap.mi.fwk.ui.menuItems.refreshclientspec"
               label="%action.name.refreshclientspec"
               mnemonic="%mnemonic.action.refreshclientspec">
         </command>
      </menuContribution>
	<!-- Delete -->
      <menuContribution
            locationURI="popup:com.sap.mi.fwk.ui.menu.modeling?after=group.partition">
         <command
               commandId="com.sap.mi.fwk.ui.commands.deletedata"
               id="com.sap.mi.fwk.ui.menuItems.deletedata"
               label="%action.name.deletedata"
               mnemonic="%mnemonic.action.deletedata">
         </command>
      </menuContribution>
   </extension>

<!-- ======================================================================================= -->
<!-- Key Bindings					                                                             -->
<!-- ======================================================================================= -->
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="com.sap.mi.fwk.ui.commands.refreshdata"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+F5">
      </key>
      <key
            commandId="com.sap.mi.fwk.ui.commands.deletedata"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+DEL">
      </key>
   </extension>

<!-- ======================================================================================= -->
<!-- Preference Initializers	                                                             -->
<!-- ======================================================================================= -->
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.sap.mi.fwk.ui.internal.preferences.EditorSaveInitializer">
      </initializer>
   </extension>

<!-- ======================================================================================= -->
<!-- Preference Pages			                                                             -->
<!-- ======================================================================================= -->
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="MoinPreferencePage"
            class="com.sap.mi.fwk.ui.internal.preferences.EditorSavePreferencePage"
            id="com.sap.mi.fwk.ui.editor.saveDialog"
            name="%_editor_save_preference_page_name">
      </page>
      
   </extension>
<extension
         point="org.eclipse.ui.decorators">
      <decorator
            class="com.sap.mi.fwk.ui.tree.TreeNodeDecorator"
            id="com.sap.mi.fwk.ui.tree.treenodedecorator"
            label="%decorator_label"
            lightweight="true"
            location="BOTTOM_LEFT"
            state="true">
         <description>
         	%decorator_desc
         </description>
         <enablement>
	           <objectClass
	                 name="com.sap.mi.fwk.ui.tree.nodes.ITreeNodeRefObject">
	           </objectClass>
         </enablement>
      </decorator>
   </extension></plugin>
   

