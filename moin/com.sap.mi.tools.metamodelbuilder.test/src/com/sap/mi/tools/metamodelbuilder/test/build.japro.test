//** B.template
NoTranslation
UseMacros(moin.buildplugin)

ImportAllLibs(PurposeLabels(compile,api))
OmitPropagationOfAllLibs(PurposeLabels(compile,api))


AddBuildTarget(DisableClassProxies)
BuildMoinMetaModel(test.sap.com,HMMOINTESTS,tc/moin/metamodel/testcases,testcases_all)
AddBuildTarget(EnableClassProxies)

<target name="DisableClassProxies">
  <MOINSetSysProp key="com.sap.tc.moin.noclassproxy" value="true" />
</target>

<target name="EnableClassProxies">
  <MOINSetSysProp key="com.sap.tc.moin.noclassproxy" value="" />
</target>
