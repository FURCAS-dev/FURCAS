# Debugging options for the org.eclipse.emf.ocl plug-in

# Turn on general debugging for the org.eclipse.emf.ocl plug-in
org.eclipse.emf.ocl/debug=true

# Trace when exceptions are caught
org.eclipse.emf.ocl/debug/exceptions/catching=false
# Trace when exceptions are thrown
org.eclipse.emf.ocl/debug/exceptions/throwing=false

# Trace when methods are entered
org.eclipse.emf.ocl/debug/methods/entering=false
# Trace when methods are exited
org.eclipse.emf.ocl/debug/methods/exiting=false
