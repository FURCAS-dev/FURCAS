# Debugging options for the org.eclipse.emf.query plug-in

# Turn on general debugging for the org.eclipse.emf.query plug-in
org.eclipse.emf.query/debug=true

# Trace when exceptions are caught
org.eclipse.emf.query/debug/exceptions/catching=false
# Trace when exceptions are thrown
org.eclipse.emf.query/debug/exceptions/throwing=false

# Trace when methods are entered
org.eclipse.emf.query/debug/methods/entering=false
# Trace when methods are exited
org.eclipse.emf.query/debug/methods/exiting=false
