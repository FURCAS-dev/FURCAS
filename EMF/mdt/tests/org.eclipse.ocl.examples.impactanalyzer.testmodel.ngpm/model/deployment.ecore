      <details key="settingDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL"/>
    </eAnnotations>
    <eSubpackages name="landscape" nsURI="http:///deployment/landscape.ecore" nsPrefix="deployment.landscape">
