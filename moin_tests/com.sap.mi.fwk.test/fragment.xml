<?xml version="1.0" encoding="utf-8"?>
<?eclipse version="3.0"?>
<fragment>

     <extension
         point="com.sap.ide.junit.insideTests">
      <testsuite
            class="com.sap.mi.fwk.test.suites.MiFwkSmokeTests"
            name="Moin in Eclipse: BI: Smoke"
            runMode="otherThread"
            runModeForNestedTests="otherThread"/>
      <testsuite
            class="com.sap.mi.fwk.test.suites.MiFwkTests"
            name="Moin in Eclipse: BI: Headless"
            runMode="otherThread"
            runModeForNestedTests="otherThread"/>
      <testsuite
            class="com.sap.mi.fwk.test.suites.MiFwkUnitTest"
            name="Moin in Eclipse: BI: Unit"/>
   </extension>
   
   <extension point="com.sap.ide.dii05.ui.dctype">
      <DCType caption="Custom Moin DC" class="com.sap.mi.fwk.test.TestDcType" type="com.sap.mi.fwk.test.TestDcType" vendor="sap.com">
         <BuildDC name="tc/bi/bp/nop" ppref="def" vendor="sap.com"/>
      </DCType>
   </extension>

   <extension
         id="TestFileBuilder"
         name="Test File Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="com.sap.mi.fwk.util.test.TestFileBuilder">
         </run>
      </builder>
   </extension>
   
   <extension
         id="TestNature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run class="com.sap.mi.fwk.test.TestNature"/>
      </runtime>
      <builder id="com.sap.mi.fwk.TestFileBuilder" />
   </extension>
    
    
</fragment>
