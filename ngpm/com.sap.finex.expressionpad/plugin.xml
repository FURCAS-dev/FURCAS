<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="com.sap.finex.expressionpad"
            name="Runlet">
      </category>
      <view
            category="com.sap.finex.expressionpad"
            class="com.sap.finex.expressionpad.views.FinexConsoleView"
            icon="icons/LittleFinex.jpg"
            id="com.sap.finex.expressionpad.views.FinexConsoleView"
            name="FINEX Console View">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="*">
         <view
               id="com.sap.finex.expressionpad.views.FinexConsoleView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.TaskList">
         </view>
      </perspectiveExtension>
   </extension>
</plugin>
